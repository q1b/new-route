{
    "name": "new-route",
    "version": "0.0.5",
    "description": "new-route is a svelte-kit, route creator cli for personal use, work in progress",
    "type": "module",
    "exports": "./dist/index.js",
    "bin": {
        "new-route": "./dist/index.js"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE",
        "CHANGELOG.md",
        "package.json"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup ./src/* --watch ",
        "format": "prettier --write .",
        "release": "changeset publish"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/q1b/new-route.git"
    },
    "keywords": [
        "svelte",
        "svelte-kit"
    ],
    "author": "q1b",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/q1b/new-route/issues"
    },
    "homepage": "https://github.com/q1b/new-route#readme",
    "devDependencies": {
        "@changesets/changelog-github": "^0.5.0",
        "@changesets/cli": "^2.27.7",
        "@types/node": "^20.14.12",
        "prettier": "3.3.3",
        "tsup": "^8.2.3",
        "typescript": "^5.5.4"
    },
    "prettier": {
        "tabWidth": 4,
        "useTabs": false
    }
}
